<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@color/Pink"
    tools:context=".SignUpActivity">

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="171dp"
        android:layout_marginEnd="314dp"
        android:layout_marginBottom="449dp"
        android:text="이름"
        android:textColor="@color/Gray"
        android:textSize="14sp"
        android:fontFamily="@font/noto_sans_kr"
        android:textFontWeight="700"
        android:lineSpacingExtra="20dp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/ed_name"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="197dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="399dp"
        android:background="@drawable/selector_sample_text"
        android:fontFamily="@font/noto_sans_kr"
        android:textFontWeight="400"
        android:ems="10"
        android:hint="이름을 입력해주세요."
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:paddingTop="12dp"
        android:paddingBottom="12dp"
        android:paddingLeft="16dp"
        android:paddingRight="181dp"
        android:singleLine="false"
        android:textStyle="normal"
        android:textAllCaps="false"
        android:textColor="@color/Gray2"
        android:lineSpacingExtra="20dp"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.503" />

    <EditText
        android:id="@+id/ed_Id2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="301dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="293dp"
        android:background="@drawable/selector_sample_text"
        android:ems="10"
        android:fontFamily="@font/noto_sans_kr"
        android:hint="아이디를 입력해주세요"
        android:inputType="textPersonName"
        android:lineSpacingExtra="20dp"
        android:minHeight="48dp"
        android:paddingLeft="16dp"
        android:paddingTop="13dp"
        android:paddingRight="126dp"
        android:paddingBottom="13dp"
        android:singleLine="false"
        android:textAllCaps="false"
        android:textColor="@color/Gray2"
        android:textFontWeight="400"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_Id2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="275dp"
        android:layout_marginEnd="259dp"
        android:layout_marginBottom="345dp"
        android:text="깃허브 아이디"
        android:textColor="@color/Gray"
        android:textSize="14sp"
        android:textStyle="normal"
        android:lineSpacingExtra="20dp"
        android:fontFamily="@font/noto_sans_kr"
        android:textFontWeight="700"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/ed_Pw2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="407dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="187dp"
        android:background="@drawable/selector_sample_text"
        android:ems="10"
        android:hint="비밀번호를 입력해주세요"
        android:inputType="textPersonName|textPassword"
        android:minHeight="48dp"
        android:paddingTop="13dp"
        android:paddingBottom="13dp"
        android:paddingLeft="16dp"
        android:paddingRight="155dp"
        android:singleLine="false"
        android:textAllCaps="false"
        android:textColor="@color/Gray2"
        android:textSize="14sp"
        android:lineSpacingExtra="20dp"
        android:textStyle="normal"
        android:fontFamily="@font/noto_sans_kr"
        android:textFontWeight="400"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_Pw2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="381dp"
        android:layout_marginEnd="288dp"
        android:layout_marginBottom="239dp"
        android:text="비밀번호"
        android:textColor="@color/Gray"
        android:textSize="14sp"
        android:textStyle="normal"
        android:lineSpacingExtra="20dp"
        android:fontFamily="@font/noto_sans_kr"
        android:textFontWeight="700"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_Join2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="248dp"
        android:layout_marginBottom="524dp"
        android:text="회원가입"
        android:textColor="@color/Gray"
        android:textSize="25sp"
        android:textStyle="normal"
        android:fontFamily="@font/noto_sans_kr"
        android:textFontWeight="700"
        android:lineSpacingExtra="36dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_Joined"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="520dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="74dp"
        android:background="@drawable/rectangle_fill_pink"
        android:paddingTop="12dp"
        android:paddingBottom="12dp"
        android:paddingRight="132dp"
        android:paddingLeft="132dp"
        android:fontFamily="@font/noto_sans_kr"
        android:textFontWeight="700"
        android:text="회원가입"
        android:textSize="15sp"
        android:textStyle="normal"
        android:textColor="@color/white"
        android:lineSpacingExtra="21dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>